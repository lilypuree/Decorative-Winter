{
  "multipart": [
    {
      "when": {
        "snowy": "false",
        "layers": "0"
      },
      "apply": {
        "model": "decorative_winter:block/dry_fern"
      }
    },
    {
      "when": {
        "snowy": "true",
        "layers": "0"
      },
      "apply": {
        "model": "decorative_winter:block/snowy_dry_fern"
      }
    },
    {
      "when": {
        "snowy": "false",
        "layers": "1|2|3|4|5|6|7|8"
      },
      "apply": {
        "model": "decorative_winter:block/dry_fern_with_snow"
      }
    },
    {
      "when": {
        "snowy": "true",
        "layers": "1|2|3|4|5|6|7|8"
      },
      "apply": {
        "model": "decorative_winter:block/snowy_dry_fern_with_snow"
      }
    },
    {
      "when": {
        "layers": "1"
      },
      "apply": {
        "model": "minecraft:block/snow_height2"
      }
    },
    {
      "when": {
        "layers": "2"
      },
      "apply": {
        "model": "minecraft:block/snow_height4"
      }
    },
    {
      "when": {
        "layers": "3"
      },
      "apply": {
        "model": "minecraft:block/snow_height6"
      }
    },
    {
      "when": {
        "layers": "4"
      },
      "apply": {
        "model": "minecraft:block/snow_height8"
      }
    },
    {
      "when": {
        "layers": "5"
      },
      "apply": {
        "model": "minecraft:block/snow_height10"
      }
    },
    {
      "when": {
        "layers": "6"
      },
      "apply": {
        "model": "minecraft:block/snow_height12"
      }
    },
    {
      "when": {
        "layers": "7"
      },
      "apply": {
        "model": "minecraft:block/snow_height14"
      }
    },
    {
      "when": {
        "layers": "8"
      },
      "apply": {
        "model": "minecraft:block/snow_block"
      }
    }
  ]
}