{
  "multipart": [
    {
      "apply": {
        "model": "decorative_winter:block/snowy_spruce_palisade_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "decorative_winter:block/snowy_spruce_palisade_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "decorative_winter:block/snowy_spruce_palisade_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "decorative_winter:block/snowy_spruce_palisade_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "decorative_winter:block/snowy_spruce_palisade_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "snowy": "true"
      },
      "apply": {
        "model": "decorative_winter:block/custom/custom_snow"
      }
    }
  ]
}